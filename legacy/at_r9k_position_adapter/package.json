  "description": "AT R9K Position Adapter",
  "homepage": "http://www.propellerhead.co.nz",
  "bugs": {
    "url": "https://propellerheadnz.atlassian.net"
  },
  "repository": {
    "type": "git",
    "url": "https://bitbucket.org/Propellerhead/at_r9k_gtfs_adapter"
  },
  "license": "UNLICENSED",
