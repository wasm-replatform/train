{
    "name": "at-dilax-apc-connector",
    "version": "1.0.0",
    "private": true,
    "description": "AT DILAX APC Connector",
    "homepage": "http://www.propellerhead.co.nz",
    "bugs": {
        "url": "https://propellerheadnz.atlassian.net"
    },
    "repository": {
        "type": "git",
        "url": "https://bitbucket.org/Propellerhead/Auckland%20Transport/at_dilax_apc_connector"
    },
    "license": "UNLICENSED",
    "author": "Propellerhead",
    "main": "src/start.ts",
    "scripts": {
        "build": "rimraf ./dist && tsc && npm run build-resource",
        "build-resource": "copyfiles ./.env \"./resource/**/*\" ./dist",
        "dev": "ts-node -r dotenv/config ./src/start.ts",
        "lint": "eslint \"src/**/*.{ts,tsx}\" --no-error-on-unmatched-pattern",
        "lint:fix": "eslint \"{src,test}/**/*.{ts,tsx}\" --no-error-on-unmatched-pattern --fix",
        "start": "cd dist || true && node -r dotenv/config src/start.js",
        "test": "NEW_RELIC_ENABLED=false jest --config node_modules/at-build-common/base/jest.json"
    },
    "eslintConfig": {
        "extends": [
            "./node_modules/at-build-common/base/eslint.json"
        ]
    },
    "dependencies": {
        "@azure/identity": "^3.2.3",
        "@azure/keyvault-secrets": "^4.7.0",
        "at-connector-common": "^15.1.3",
        "at-realtime-common": "^6.8.15",
        "dotenv": "^16.3.1",
        "newrelic": "^11.6.0"
    },
    "devDependencies": {
        "@types/newrelic": "^9.14.3",
        "at-build-common": "^10.3.3",
        "jest": "^29.7.0",
        "ts-jest": "^29.1.1",
        "typescript": "^5.3.2"
    },
    "engines": {
        "node": "~18"
    }
}
